{
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "prettier.semi": false,
  "terminal.integrated.copyOnSelection": true,
  "vim.easymotion": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "window.zoomLevel": 0
}